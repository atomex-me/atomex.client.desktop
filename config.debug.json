{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Atomex.Client.Core",
      "Beacon.Sdk"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithCaller"
    ]
  },
  "Services": {
    "MainNet": {
      "Exchange": {
        "Url": "wss://atomex.me/exchange"
      },
      "MarketData": {
        "Url": "wss://atomex.me/marketdata"
      }
    },
    "TestNet": {
      "Exchange": {
        "Url": "wss://test.atomex.me/exchange"
      },
      "MarketData": {
        "Url": "wss://test.atomex.me/marketdata"
      }
    }
  }
}